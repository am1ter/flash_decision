# [RUN]
# For `PYTHONPATH` used 2 independant env vars in format: local:docker
PYTHONPATH="backend:app"

# [ENV]
ENVIRONMENT="production"
DEBUG_MODE="False"
LOG_LEVEL="INFO"
TZ="UTC"

# [HTTP]
BACKEND_PROTOCOL="http"
BACKEND_HOST="0.0.0.0"
BACKEND_PORT=8001
BACKEND_API_PREFIX="api/v1"
FRONTEND_URL="http://0.0.0.0:8000/"

# [DB]
# If you use external database uncomment `DB_HOST` parameter and edit it
# DB_HOST="localhost"
DB_PORT=5432

# [CACHE]
CACHE_REDIS_PORT=6379

# [HEALTHCHECK]
HEALTHCHECK_DISABLE="False"
HEALTHCHECK_URL="support/healthcheck"
HEALTHCHECK_INTERVAL="15m"

# [JWT]
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=1440
