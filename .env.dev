# [RUN]
# For `PYTHONPATH` used 2 independant env vars in format: vscode:poetry(docker)
PYTHONPATH="backend:app"
PYDEVD_DISABLE_FILE_VALIDATION=1

# [ENV]
ENVIRONMENT="development"
DEBUG_MODE="False"
TZ="UTC"

# [HTTP]
# Customise `BACKEND_URL` parameter in the case when you need to use reverse proxy (nginx)
BACKEND_URL="http://0.0.0.1:8001/api/v1"
BACKEND_HOST="0.0.0.0"
BACKEND_PORT=8001

# [DB]
# If you use external database uncomment `DB_HOST` parameter and edit it
# DB_HOST="localhost"
DB_PORT=5432
DB_PASS="flash!Pass"
